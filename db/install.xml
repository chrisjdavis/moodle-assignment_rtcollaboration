<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/assignment/type/rtcollaboration/db" VERSION="20110223" COMMENT="XMLDB file for Moodle mod/assignment/type/rtcollaboration"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="assignment_rtcollab_text" COMMENT="Shared text for an assignment" NEXT="assignment_rtcollab_view">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="assignment"/>
        <FIELD NAME="assignment" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="groupid"/>
        <FIELD NAME="groupid" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="assignment" NEXT="text"/>
        <FIELD NAME="text" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="groupid" NEXT="locked"/>
        <FIELD NAME="locked" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="text" NEXT="timelocked"/>
        <FIELD NAME="timelocked" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="locked" NEXT="timemodified"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="timelocked"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="assignment-group"/>
        <KEY NAME="assignment-group" TYPE="unique" FIELDS="assignment, groupid" PREVIOUS="primary"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="id-locked" UNIQUE="false" FIELDS="id, locked" NEXT="timelocked"/>
        <INDEX NAME="timelocked" UNIQUE="false" FIELDS="timelocked" PREVIOUS="id-locked"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="assignment_rtcollab_view" COMMENT="Default comment for the table, please edit me" PREVIOUS="assignment_rtcollab_text">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="assignment"/>
        <FIELD NAME="assignment" TYPE="int" LENGTH="10" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="userid" NEXT="shadow"/>
        <FIELD NAME="shadow" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="assignment" NEXT="backup_shadow"/>
        <FIELD NAME="backup_shadow" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" ENUM="false" PREVIOUS="shadow" NEXT="shadow_client_version"/>
        <FIELD NAME="shadow_client_version" TYPE="int" LENGTH="6" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="backup_shadow" NEXT="shadow_server_version"/>
        <FIELD NAME="shadow_server_version" TYPE="int" LENGTH="6" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="shadow_client_version" NEXT="backup_shadow_server_version"/>
        <FIELD NAME="backup_shadow_server_version" TYPE="int" LENGTH="6" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="shadow_server_version"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" NEXT="user-assignment"/>
        <KEY NAME="user-assignment" TYPE="unique" FIELDS="userid, assignment" PREVIOUS="primary"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>